{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "references": [
    { "path": "../../packages/@mimo/types" },
    { "path": "../../packages/@mimo/engine" }
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "baseUrl": "."
  }
}
