{
  "name": "@mimo/llm",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Mimo LLM - Language model integration",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "check-types": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.4",
    "@ai-sdk/gateway": "ai-v5",
    "@ai-sdk/google": "^1.0.4",
    "@ai-sdk/openai": "^3.0.23",
    "@anthropic-ai/sdk": "^0.32.1",
    "@google/generative-ai": "^0.21.0",
    "@mimo/types": "workspace:*",
    "ai": "^6.0.64",
    "openai": "^4.73.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@vitest/coverage-v8": "^3.2.4",
    "tsdown": "^0.20.0-beta.4",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
